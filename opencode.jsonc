    "share": "disabled",
    "autoupdate": true,
    "permission": {
        "bash": {
            "*": "ask",
        },
        "edit": {
            "**/*.env*": "deny",
            "**/*.key": "deny",
            "**/*.secret": "deny",
            "node_modules/**": "deny",
            ".git/**": "deny",
        },
        "read": {
            "**/*.env*": "deny",
            "**/*.key": "deny",
            "**/*.secret": "deny",
            "node_modules/**": "deny",
            ".git/**": "deny",
        },
    },
    "command": {
        "snyk": {
            "template": "Run `snyk test` to check for security vulnerabilities.",
            "description": "Run Snyk security test",
            "agent": "build",
        },
    },
    "mcp": {
        "snyk": {
            "type": "local",
            "enabled": true,
            "command": [
                "snyk",
                "mcp",
                "-t",
                "stdio"
            ],
        },
    },
}
